#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <string>
using namespace std;


int main() {
    int price;
    string title;
    string author;

    
    getline(cin, title);

    getline(cin, author);

    cin >> price;

    cout << "Title: " << title << '\n';
    cout << "Author: " << author << '\n';
    cout << "Price: " << price;

    return 0;
}
